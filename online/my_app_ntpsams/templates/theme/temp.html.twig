<!DOCTYPE html>
<html lang="en">
{% set version = func_pageinfo()['versioncss'] %}
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta name="description" content="">
    <meta name="keywords" content="">


    <link href="{{ asset('assets/img/pageinfo/'~func_pageinfo()['favory']) }}" rel="icon">
    <link href="{{ asset('assets/img/pageinfo/'~func_pageinfo()['favory']) }}" rel="apple-touch-icon">


    <link href="{{ asset('') }}assets/vendor/bootstrap/css/bootstrap.min.css?v={{ version }}" rel="stylesheet">
    <link href="{{ asset('') }}assets/vendor/bootstrap-icons/bootstrap-icons.css?v={{ version }}" rel="stylesheet">
    <link href="{{ asset('') }}assets/vendor/aos/aos.css?v={{ version }}" rel="stylesheet">
    <link href="{{ asset('') }}assets/vendor/swiper/swiper-bundle.min.css?v={{ version }}" rel="stylesheet">
    <link href="{{ asset('') }}assets/vendor/glightbox/css/glightbox.min.css?v={{ version }}" rel="stylesheet">

    <!-- Main CSS File -->
    {#    <link href="{{ asset('') }}assets/css/main.css" rel="stylesheet">#}
    <link href="{{ asset('') }}assets/css/main_blue.css?v={{ version }}" rel="stylesheet">
    <link href="{{ asset('') }}assets/css/style_data_section.css?v={{ version }}" rel="stylesheet">




    <style>
        .section{
            padding: 50px 0 !important;
        }

        .form-check-input[type="radio"]{
            border:solid 1px #000000;
        }


        @media (max-width: 767px){

            .shop_cls{
                padding-top:-20rem !important;
            }

        }



    </style>


    {% block stylesheets %}
    {% endblock %}

    {% block javascripts %}
    {% endblock %}
</head>

<body class="index-page">

{% include 'theme/menu.html.twig' %}

<main class="main">

    {% block body %}{% endblock %}


</main>


{% include 'theme/footer_txt.html.twig' %}

<!-- Scroll Top -->
<a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

<!-- Preloader -->
{#<div id="preloader"></div>#}
<!-- Vendor JS Files -->
<script src="{{ asset('') }}assets/vendor/bootstrap/js/bootstrap.bundle.min.js?v={{ version }}"></script>
<script src="{{ asset('') }}assets/vendor/aos/aos.js?v={{ version }}"></script>
<script src="{{ asset('') }}assets/vendor/swiper/swiper-bundle.min.js?v={{ version }}"></script>
<script src="{{ asset('') }}assets/vendor/purecounter/purecounter_vanilla.js?v={{ version }}"></script>
<script src="{{ asset('') }}assets/vendor/imagesloaded/imagesloaded.pkgd.min.js?v={{ version }}"></script>
<script src="{{ asset('') }}assets/vendor/isotope-layout/isotope.pkgd.min.js?v={{ version }}"></script>
<script src="{{ asset('') }}assets/vendor/glightbox/js/glightbox.min.js?v={{ version }}"></script>

<!-- Main JS File -->
<script src="{{ asset('') }}assets/js/main.js"></script>

</body>

</html>
